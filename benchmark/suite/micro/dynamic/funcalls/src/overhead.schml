(letrec ([id : Dyn (lambda ((x : Dyn)) x)])
  (let ([iters : Dyn (read-int)]
        [acc : Dyn (gbox 0)])
    (letrec ([run-test : Dyn (lambda (i acc) i)])
      (begin
        (timer-start)
        (repeat (i 0 iters)
                (gbox-set! acc (run-test i (gunbox acc))))
        (timer-stop)
        (timer-report)
        (gunbox acc)))))
