;;; TAK -- A vanilla version of the TAKeuchi function.

(define (tak [x : Int] [y : Int] [z : Int]) : Int
  (if (>= y x)
      z
      (tak (tak (- x 1) y z)
           (tak (- y 1) z x)
           (tak (- z 1) x y))))

(define (run-benchmark) : Unit
  (let ([x : Int (read-int)])
    (let ([y : Int (read-int)])
      (let ([z : Int (read-int)])
        (begin (print-int (tak x y z))
               (display-char #\newline))))))

(time (run-benchmark))
