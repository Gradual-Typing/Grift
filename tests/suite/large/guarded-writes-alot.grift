;; Perform a ton of Reads and Writes
(let ([b (gbox 0)])
  (begin
    (timer-start)
    (repeat (i 0 100)
     (begin
      (repeat (j 0 i)
       (gbox-set! b (+ (gunbox b) i)))
      (repeat (j 0 (gunbox b))
       (gbox-set! b (- (gunbox b) 1)))))
    (timer-stop)
    (timer-report)
    (gunbox b)))
 
       
