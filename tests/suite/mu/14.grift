;; This function test iso-recusive type equality
;; in multiple ways
;; It actually runs the example
(define (ones) : (Rec X (Tuple Int (-> X)))
  (tuple 1 ones))

(define (stream-ref s i)
  (if (< i 1)
      (tuple-proj (s) 0)
      (stream-ref (tuple-proj (s) 1) (- i 1))))

(ann (stream-ref (ann ones (Rec X (-> (Tuple Dyn X))))
                 42)
     Int)

