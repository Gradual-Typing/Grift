(let ((mre : Dyn #f))
 (let ((dynref : (GRef Dyn) (gbox mre)))
  (let ((boolref : (GRef Bool) dynref)
        (intref  : (GRef Int)  dynref))
   (begin
    (if (gunbox boolref)
        (gbox-set! intref 6)
        (gbox-set! intref 7))
    (gbox-set! intref (* (gunbox intref) 6))
    (: (gunbox dynref) Int)))))
